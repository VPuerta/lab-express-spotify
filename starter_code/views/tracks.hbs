{{#each tracks}}
<div class="container">
    <div class="row header-row">
        <span class="col-lg-8">Title</span>
        <span class="col-lg-4">Listen</span>
    </div>



    <div class="row track-item">
        <div class="col-lg-8">
            <p>{{this.name}}</p>
        </div>
        <div class="col-lg-4">
            <audio controls src="{{this.preview_url}}" type="audio/mpeg" type="audio/ogg">
                Your browser does not support the
                <code>audio</code> element.
            </audio>
        </div>
    </div>
</div>



{{/each}}